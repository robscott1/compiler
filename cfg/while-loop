digraph {
	31 [label="%0 = alloca i32"]
	dd [label="%0 = load i32, i32* 4"]
	31 -> dd
	31 [label="%1 = icmp lt i1 %0, 4
br i1 %1 label 66 label d2"]
	dd -> 31
	66 [label="%2 = alloca i32"]
	31 -> 66
	41 [label="br label 31"]
	66 -> 41
	41 -> 31
	d2 [label="ret i32 2"]
	31 -> d2
	"3d" [label=""]
	d2 -> "3d"
}
