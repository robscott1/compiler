digraph {
	77 [label="%0 = alloca %struct.A*
%1 = alloca i32"]
	bf [label="%2 = call i32 f ()
%3 = icmp lt i1 0, %2
br i1 %3 label 7a label 83"]
	77 -> bf
	"7a" [label="ret i32 4"]
	bf -> "7a"
	80 [label=""]
	"7a" -> 80
	83 [label="%4 = getelementptr %struct.A*, %struct.A** %0 i1 0, i32 0
