digraph {
	"2b" [label="%1 = alloca i32"]
	b1 [label="%1 = load i32, i32* 3
%2 = icmp gt i1 %1, 0
br i1 %2 label fe label a5"]
	"2b" -> b1
	fe [label="ret i32 2"]
	b1 -> fe
	"4f" [label=""]
	fe -> "4f"
	a5 [label="%3 = icmp lt i1 %1, 0
br i1 %3 label af label 45"]
	b1 -> a5
	af [label="%4 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([3 x i8], [3 x i8]* @.str, i32 0, i32 0), i32 1)"]
	a5 -> af
	45 [label="ret i32 0"]
	a5 -> 45
	45 -> "4f"
}
