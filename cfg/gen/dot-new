digraph {
	bc [label="%1 = alloca %struct.A*
%2 = alloca i32"]
	c5 [label="%2 = load i32, i32* 4
%3 = bitcast %struct.A** %1 to i8*
call void @free(i8* %1)
%4 = call i8* @malloc(i32 9)
%5 = bitcast i8* %4 to %struct.A*
%6 = getelementptr %struct.A*, %struct.A** %5 i1 0, i32 0
ret i32 %6"]
	bc -> c5
	"2b" [label=""]
	c5 -> "2b"
}
