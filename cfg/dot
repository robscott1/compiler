digraph {
	55 [label="%0 = alloca %struct.A
%1 = alloca i32"]
	60 [label="%2 = call i32 f ()
%2 = call i32 f ()
%3 = icmp lt i1 0, %2
br i1 %3 label ff label ea"]
	55 -> 60
	ff [label="ret i32 3"]
	60 -> ff
	37 [label=""]
	ff -> 37
	ea [label="%4 = getelementptr %struct.A, %struct.A* %0 i1 0, i32 0
ret i32 %4"]
	60 -> ea
	ea -> 37
}
