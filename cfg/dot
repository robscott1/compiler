digraph {
	ea [label="%0 = alloca %struct.A
%1 = alloca i32"]
	52 [label="%2 = call i32 f ()
%3 = icmp lt i1 0, %2
br i1 %3 label 7d label 6d"]
	ea -> 52
	"7d" [label="ret i32 3"]
	52 -> "7d"
	"6d" [label="%4 = getelementptr %struct.A, %struct.A* %0 i1 0, i32 0
ret i32 %4"]
	"7d" -> "6d"
	f2 [label=""]
	"6d" -> f2
	"7d" -> f2
	52 -> "6d"
}
